<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:dither="true"
            android:shape="oval"
            android:useLevel="false">
            <solid android:color="@color/transparent" />
            <stroke
                android:width="1dp"
                android:color="@color/colorAccent" />
            <size
                android:width="16dp"
                android:height="16dp" />
        </shape>
    </item>

</layer-list>